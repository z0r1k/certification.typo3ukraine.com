<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.sheet_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<what_to_display>
						<TCEforms>
							<label>LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.what_to_display</label>
							<config>
								<type>select</type>
								<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="0">LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.view_dynamic</numIndex>
									<numIndex index="1">DYNAMIC</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="0">LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.view_static</numIndex>
									<numIndex index="1">STATIC</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="0">LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.view_static_separate</numIndex>
									<numIndex index="1">STATIC_SEPARATE</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="0">LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.view_search</numIndex>
									<numIndex index="1">SEARCH</numIndex>
								</numIndex>
								</items>
								<maxitems>100</maxitems>
								<size>4</size>
								<multiple>1</multiple>
								<selectedListStyle>width:300px</selectedListStyle>
								<itemListStyle>width:300px</itemListStyle>
							</config>
						</TCEforms>
					</what_to_display>
					<pages>
						<TCEforms>
						<exclude>1</exclude>
						<label>LLL:EXT:lang/locallang_general.php:LGL.startingpoint</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>3</size>
							<maxitems>22</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
						</TCEforms>
					</pages>
					<recursive>
						<TCEforms>
						<label>LLL:EXT:lang/locallang_general.php:LGL.recursive</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.1</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.2</numIndex>
										<numIndex index="1">2</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.3</numIndex>
										<numIndex index="1">3</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.4</numIndex>
										<numIndex index="1">4</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.5</numIndex>
										<numIndex index="1">250</numIndex>
									</numIndex>
								</items>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</recursive>
					<enableRatings>
						<TCEforms>
							<label>LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.enableRatings</label>
							<displayCond>EXT:ratings:LOADED:true</displayCond>
							<config>
								<type>check</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
								</items>
								<default>0</default>
							</config>
						</TCEforms>
					</enableRatings>
				</el>
			</ROOT>
		</sDEF>
		<sCATEGORIES>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.sheet_categories</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<categoryMode>
						<TCEforms>
							<label>LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.select_deselect_categories</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.category_selection_showAll</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.category_selection_showSelected</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.category_selection_DontShowSelected</numIndex>
										<numIndex index="1">-1</numIndex>
									</numIndex>
								</items>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</categoryMode>
					<categorySelection>
						<TCEforms>
							<label>LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.category_selection</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="100">
										<numIndex index="0">LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.category_selection_notCategorized</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
								</items>
								<foreign_table>tx_irfaq_cat</foreign_table>
								<foreign_table_where>ORDER BY tx_irfaq_cat.uid</foreign_table_where>
								<foreign_table_where>AND tx_irfaq_cat.pid=###STORAGE_PID### ORDER BY tx_irfaq_cat.uid</foreign_table_where>
								<minitems>0</minitems>
								<maxitems>20</maxitems>
								<size>5</size>
								<selectedListStyle>width:150px</selectedListStyle>
								<itemListStyle>width:150px</itemListStyle>
								<allowNonIdValues>1</allowNonIdValues>
							</config>
							<wizards type="array">
								<_PADDING>1</_PADDING>
								<add type="array">
									<type>script</type>
									<title>Add Category</title>
									<script>wizard_add.php</script>
									<icon>add.gif</icon>
									<params type="array">
										<table>tx_irfaq_cat</table>
										<pid>###STORAGE_PID###</pid>
										<setValue>set</setValue>
									</params>
								</add>
							</wizards>
						</TCEforms>
					</categorySelection>
				</el>
			</ROOT>
		</sCATEGORIES>
		<sSEARCH>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.sheet_search</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<searchPid>
						<TCEforms>
						<exclude>1</exclude>
						<label>LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.searchPid</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
						</TCEforms>
					</searchPid>
					<emptySearchAtStart>
						<TCEforms>
						<label>LLL:EXT:irfaq/lang/locallang_db.xml:tx_irfaq.pi_flexform.emptySearchAtStart</label>
						<config>
							<type>check</type>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="0"></numIndex>
									<numIndex index="1">1</numIndex>
								</numIndex>
							</items>
						</config>
						</TCEforms>
					</emptySearchAtStart>
				</el>
			</ROOT>
		</sSEARCH>
	</sheets>
</T3DataStructure>